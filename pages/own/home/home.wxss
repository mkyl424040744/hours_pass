.overlay {
   position: fixed;
   top: 0;
   left: 0;
   width: 100%;
   height: 100%;
   background-color: rgba(0, 0, 0, 0.7);
}

.z-2 {
   z-index: 2;
}

.centered {
   display: flex;
   justify-content: center;
   align-items: center;
   width: 100%;
   height: 100%;
   position: absolute;
}

.cuIcon-right:before {
   content: "\279C";
}

.padding-a {
   padding: 40rpx;
}

.bg {
   background-color: #999393;
}

.h_lignment {
   display: flex;
   flex-direction: row;
   align-items: center;
}

.imgHead {
   overflow:hidden;
   width: 140rpx;
   height: 140rpx;
   border-radius: 50%;
   margin:0 40rpx 0 0;
}
