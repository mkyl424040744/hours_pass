<van-tabbar
  active="{{ active4 }}"
  data-key="active4"
  custom-class="tabbar"
  active-color="#07c160"
  inactive-color="#aaa"
  safe-area-inset-bottom="{{ false }}"
>
  <van-tabbar-item
    id="0"
    url="/pages/activity/home/home"
    data-url="/pages/activity/home/home"
    data-switch-tab="{{ true }}"
    bind:click="onClick"
  >
    <image
      slot="icon"
      src="{{ icon1.normal }}"
      mode="aspectFit"
      style="width: 25px; height: 25px;"
    />
    <image
      slot="icon-active"
      src="{{ icon1.active }}"
      mode="aspectFit"
      style="width: 25px; height: 25px;"
    />
    活动
  </van-tabbar-item>
  <van-tabbar-item
    id="1"
    url="/pages/consultation/home/home"
    data-url="/pages/consultation/home/home"
    data-switch-tab="{{ true }}"
    bind:click="onClick"
  >
    <image
      slot="icon"
      src="{{ icon2.normal }}"
      mode="aspectFit"
      style="width: 25px; height: 25px;"
    />
    <image
      slot="icon-active"
      src="{{ icon2.active }}"
      mode="aspectFit"
      style="width: 25px; height: 25px;"
    />
    学时咨询
  </van-tabbar-item>
  <van-tabbar-item
    id="2"
    url="/pages/own/home/home"
    data-url="/pages/own/home/home"
    data-switch-tab="{{ true }}"
    bind:click="onClick"
  >
    <image
      slot="icon"
      src="{{ icon3.normal }}"
      mode="aspectFit"
      style="width: 25px; height: 25px;"
    />
    <image
      slot="icon-active"
      src="{{ icon3.active }}"
      mode="aspectFit"
      style="width: 25px; height: 25px;"
    />
    我的
  </van-tabbar-item>
</van-tabbar>
